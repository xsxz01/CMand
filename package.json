{
  "name": "cmand",
  "displayName": "CMand",
  "description": "CMand中文编程VSCode插件",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cmand.打开项目",
        "title": "打开项目"
      },
      {
        "command": "cmand.创建新项目",
        "title": "创建新项目"
      },
      {
        "command": "cmand.构建",
        "title": "构建"
      },
      {
        "command": "cmand.调试",
        "title": "调试"
      },
      {
        "command": "cmand.配置",
        "title": "配置"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "cmand-view",
          "title": "CMand",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "cmand-view": [
        {
          "type": "webview",
          "id": "cmand.webview",
          "name": "CMand面板",
          "contextualTitle": "CMand操作面板"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.5",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.36"
  }
}
